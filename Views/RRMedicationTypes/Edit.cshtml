@model RRPatients.Models.MedicationType

@{
    ViewData["Title"] = "Edit Medication Type";
}

<h1>Edit Medication Type</h1>

<h4>MedicationType</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MedicationTypeId" class="control-label"></label>
                <input asp-for="MedicationTypeId" type="text" disabled class="form-control" />
